<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- App bar颜色 -->
    <color name="colorPrimary">#F9F9F9</color>
    <!-- 状态栏颜色(Lollipop - API Level 21 以上) -->
    <color name="colorPrimaryDark">#F0F0F0</color>
    <!-- 控件的主色调(光标颜色、checkbox背景色等) -->
    <color name="colorAccent">#FF5256</color>
    <!-- 灰色背景色 -->
    <color name="colorGrey">#F5F5F5</color>
    <!-- 标题栏目 -->
    <color name="colorTitleBar">#FFFFFF</color>
    <!-- 底部导航栏 -->
    <color name="colorTabBar">#F7F7F7</color>
    <!-- 透明灰色背景 -->
    <color name="transparentGrey">#A0000000</color>

    <!-- 字体颜色 -->
    <color name="text_dark">#333333</color>
    <color name="text_black">#666666</color>
    <color name="text_grey">#999999</color>
    <color name="text_silver">#CCCCCC</color>
    <color name="text_blue">#3BB0FF</color>
    <color name="text_red">#F95A28</color>
    <color name="text_green">#30DBA6</color>
    <color name="text_white">#FFFFFF</color>

    <!-- EditText-hint-灰色 -->
    <color name="hint_grey">#CCCCCC</color>

    <!-- 条件筛选控件 -->
    <color name="filter_tab_background">@color/colorAccent</color>
    <color name="filter_tab_line">@color/transparent</color>
    <color name="filter_tab_text_normal">@color/text_white</color>
    <color name="filter_tab_text_selected">@color/text_white</color>
    <color name="filter_item_normal">@color/text_black</color>
    <color name="filter_item_selected">@color/colorAccent</color>

    <!-- 灰色线 -->
    <color name="line_grey">#DDDDDD</color>

    <!-- item选中背景色 -->
    <color name="item_select_bg">#CCFFFF</color>

    <!-- button-enable -->
    <color name="enable_bg">#CF261F</color>
    <!-- button-disable -->
    <color name="disable_bg">#BDBDBD</color>
    <!-- button-pressed -->
    <color name="press_bg">#9A1F24</color>

    <!-- 占位图文字颜色 -->
    <color name="placeholder_text_color">#999999</color>
</resources>
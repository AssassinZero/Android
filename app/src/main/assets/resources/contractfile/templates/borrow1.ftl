    }
    
    @font-face {
      font-family: "\@宋体";
      panose-1: 2 1 6 0 3 1 1 1 1 1;
    }
    /* Style Definitions */
    
    p.MsoNormal,
    li.MsoNormal,
    div.MsoNormal {
      margin: 0cm;
      margin-bottom: .0001pt;
      text-align: justify;
      text-justify: inter-ideograph;
      font-size: 10.5pt;
      font-family: "Calibri", "sans-serif";
    }
    
